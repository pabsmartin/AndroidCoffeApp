<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="es.deusto.coffe_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".activities.AmountActivity"/>
        <activity android:name=".activities.OverallViewActivity" />
        <activity android:name=".activities.AnotherOneActivity" />
        <activity android:name=".activities.ConfirmDenyActivity" />
        <activity android:name=".activities.ProductivityActivity" />
        <activity android:name=".activities.MoodActivity" />
        <activity android:name=".activities.CoffeeSizeActivity" />
        <activity
            android:name=".activities.LandingActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>